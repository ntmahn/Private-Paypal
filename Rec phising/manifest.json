{
    "manifest_version": 3,
    "name": "Phishing Checker",
    "version": "1.0",
    "description": "Kiểm tra URL lừa đảo",
    "icons": {
      "48": "icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab",
    "tabs"
    ],
    "host_permissions": [
      "*://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  